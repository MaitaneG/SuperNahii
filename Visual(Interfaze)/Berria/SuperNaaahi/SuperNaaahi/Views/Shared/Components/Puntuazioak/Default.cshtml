@{
    @model IEnumerable<SuperNaaahi.Models.PuntuazioaModel>
}
<table class="table table-striped">
    <thead>
        <tr>
            <th>Langilea</th>
            <th>Puntuazioa</th>
            <th>Data</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr id="row-@item.Id">
                <th>@Html.DisplayFor(modelItem => item.Jokalaria)</th>
                <th>@Html.DisplayFor(modelItem => item.Puntuazioa)</th>
                <th>@Html.DisplayFor(modelItem => item.CreateDate)</th>
            </tr>
        }
    </tbody>
</table>
